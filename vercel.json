{
    "version": 2,
    "builds": [
        {
            "src": "frontend/artiction/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}

